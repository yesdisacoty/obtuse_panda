<%= form_for @blog, html: {multipart: true} do |f| %>

    <p><%= f.label :photo %></p>
    <p><%= f.file_field :blog_photo %></p>
    
    <p><%= f.label :title %></p>
    <p><%= f.text_field :title %></p>
    
    <p><%= f.label :body %></p>
    <p><%= f.text_area :body %></p>
    
    <p><%= f.submit %></p>
<% end %>